        "import hashlib\n",
        "\n",
        "text = input(\"Enter your text : \")\n",
        "hash_object = hashlib.md5(text.encode())\n",
        "print (\"the hexadecimal equivalent of hash is :\")\n",
        "md5_hash = hash_object.hexdigest()\n",
        "print(md5_hash)\n",
        "\n"
